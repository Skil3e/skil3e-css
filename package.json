{
  "name": "skil3e-css",
  "version": "1.0.9",
  "author": "Skil3e",
  "description": "A simple utility CSS Lib",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "dev": "node-sass src/skil3e.scss > src/_debug/skil3e.css --output-style expanded",
    "build": "node-sass src/skil3e.scss > dist/skil3e.min.css --output-style compressed && npx postcss dist/skil3e.min.css --use autoprefixer -d dist/ --no-map",
    "build-basic": "node-sass src/basic.scss > dist/skil3e-basic.min.css --output-style compressed && npx postcss dist/skil3e-basic.min.css --use autoprefixer -d dist/ --no-map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skil3e/skil3e-css.git"
  },
  "bugs": {
    "url": "https://github.com/Skil3e/skil3e-css/issues"
  },
  "homepage": "https://github.com/Skil3e/skil3e-css#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "browserslist": "^4.12.0",
    "node-sass": "^4.14.1",
    "postcss-cli": "^7.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "since 2010"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  }
}
